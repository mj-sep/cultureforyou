<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#000627">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <ImageButton
            android:id="@+id/st_list"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:backgroundTint="#00FFFFFF"
            app:srcCompat="@drawable/str_list"
            app:layout_constraintHorizontal_chainStyle="spread"
            tools:layout_editor_absoluteX="312dp"
            tools:layout_editor_absoluteY="646dp" />

        <ImageButton
            android:id="@+id/st_start"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:backgroundTint="#00FFFFFF"
            app:srcCompat="@drawable/str_start"
            tools:layout_editor_absoluteX="200dp"
            tools:layout_editor_absoluteY="646dp" />

        <ImageButton
            android:id="@+id/st_heart"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:backgroundTint="#00FFFFFF"
            app:srcCompat="@drawable/str_heart"
            tools:layout_editor_absoluteX="20dp"
            tools:layout_editor_absoluteY="652dp" />

        <ImageButton
            android:id="@+id/st_next"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:backgroundTint="#00FFFFFF"
            app:srcCompat="@drawable/str_next"
            tools:layout_editor_absoluteX="254dp"
            tools:layout_editor_absoluteY="646dp" />

        <ImageButton
            android:id="@+id/st_back"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:backgroundTint="#00FFFFFF"
            app:srcCompat="@drawable/str_back"
            tools:layout_editor_absoluteX="142dp"
            tools:layout_editor_absoluteY="646dp" />
    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.constraintlayout.widget.ConstraintLayout>